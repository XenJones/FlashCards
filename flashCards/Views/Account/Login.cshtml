@model User

<form method="post">
    <div class="form-group">
        <label asp-for="Email">Email</label>
        <input asp-for="Email" class="form-control" type="text" id="Phone"/>
    </div>
    <div class="form-group">
        <label asp-for="PasswordHash">Password</label>
        <input asp-for="PasswordHash" class="form-control" type="password" id="Password"/>
    </div>
    <button type="submit" class="btn btn-primary m-1">Register</button>
</form>


<button class="btn btn-secondary m-1" asp-action="Logout">Logout</button>
